// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("NEXT_PUBLIC_DEV_DATABASE_URL")
}

model Note {
  id        String @id @default(uuid())
  title     String
  body      String
  createdAt String
}
